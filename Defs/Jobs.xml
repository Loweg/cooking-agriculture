<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<JobDef>
		<defName>CA_EmptyProcessor</defName>
		<driverClass>CookingAgriculture.Processors.JobDriver_EmptyProcessor</driverClass>
		<reportString>taking product out of TargetA.</reportString>
		<suspendable>false</suspendable>
	</JobDef>
	<JobDef>
		<defName>CA_FillProcessor</defName>
		<driverClass>CookingAgriculture.Processors.JobDriver_FillProcessor</driverClass>
		<reportString>filling TargetA.</reportString>
		<suspendable>false</suspendable>
	</JobDef>
	<WorkGiverDef>
		<defName>CA_EmptyProcessor</defName>
		<label>empty processor</label>
		<giverClass>CookingAgriculture.Processors.WorkGiver_EmptyProcessor</giverClass>
		<workType>Hauling</workType>
		<verb>take product</verb>
		<gerund>taking product from</gerund>
		<requiredCapacities>
			<li>Manipulation</li>
		</requiredCapacities>
	</WorkGiverDef>
	<WorkGiverDef>
		<defName>CA_FillProcessor</defName>
		<label>fill processor</label>
		<giverClass>CookingAgriculture.Processors.WorkGiver_FillProcessor</giverClass>
		<workType>Hauling</workType>
		<verb>fill processor</verb>
		<gerund>filling</gerund>
		<requiredCapacities>
			<li>Manipulation</li>
		</requiredCapacities>
	</WorkGiverDef>

	<JobDef>
		<defName>CA_TakeFromSaltPan</defName>
		<reportString>raking salt in TargetA.</reportString>
		<suspendable>false</suspendable>
	</JobDef>

	<WorkGiverDef>
		<defName>CA_DoBillsMill</defName>
		<label>mill</label>
		<giverClass>WorkGiver_DoBill</giverClass>
		<workType>Cooking</workType>
		<priorityInType>90</priorityInType>
		<fixedBillGiverDefs>
			<li>CA_MillManual</li>
		</fixedBillGiverDefs>
		<verb>mill</verb>
		<gerund>grinding flour at</gerund>
		<requiredCapacities>
			<li>Manipulation</li>
		</requiredCapacities>
		<prioritizeSustains>true</prioritizeSustains>
	</WorkGiverDef>

	<JobDef>
		<defName>CA_FeedYeastCulture</defName>
		<driverClass>CookingAgriculture.JobDriver_FeedYeastCulture</driverClass>
		<reportString>feeding TargetA.</reportString>
		<allowOpportunisticPrefix>true</allowOpportunisticPrefix>
	</JobDef>
	<WorkGiverDef>
		<defName>CA_FeedYeastCulture</defName>
		<label>feed yeast cultures</label>
		<giverClass>CookingAgriculture.WorkGiver_FeedYeastCulture</giverClass>
		<workType>Hauling</workType>
		<verb>feed yeast culture</verb>
		<gerund>feeding</gerund>
		<requiredCapacities>
			<li>Manipulation</li>
		</requiredCapacities>
		<canBeDoneByMechs>false</canBeDoneByMechs>
	</WorkGiverDef>
</Defs>