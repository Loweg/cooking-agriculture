	<!-- Mills -->
	<ThingDef ParentName="BenchBase">
		<defName>CA_MillManual</defName>
		<label>hand mill</label>
		<thingClass>Building_WorkTable</thingClass>
		<description>Hand mill.</description>
		<graphicData>
			<texPath>Things/Building/Production/CA_Mill/CA_MillManual</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(.9,.9)</drawSize>
		</graphicData>
		<building>
			<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
		</building>
		<costList>
			<WoodLog>10</WoodLog>
		</costList>
		<costStuffCount>2</costStuffCount>
		<stuffCategories Inherit="False">
			<li>CA_ChunkStone</li>
		</stuffCategories>
		<inspectorTabs>
			<li>ITab_Bills</li>
		</inspectorTabs>
		<placeWorkers>
			<li>PlaceWorker_PreventInteractionSpotOverlap</li>
		</placeWorkers>
		<researchPrerequisites>
			<li>CA_MillingBasic</li>
		</researchPrerequisites>
		<statBases>
			<Flammability>0</Flammability>
			<MaxHitPoints>120</MaxHitPoints>
			<WorkToBuild>500</WorkToBuild>
		</statBases>
		<altitudeLayer>Building</altitudeLayer>
		<designationCategory>Production</designationCategory>
		<fillPercent>0.5</fillPercent>
		<hasInteractionCell>True</hasInteractionCell>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<passability>PassThroughOnly</passability>
		<pathCost>70</pathCost>
		<size>(1,1)</size>
		<surfaceType>Item</surfaceType>
	</ThingDef>

	<ThingDef Name="CA_BigMillBase" ParentName="BuildingBase" Abstract="True">
