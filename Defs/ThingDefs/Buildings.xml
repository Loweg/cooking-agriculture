			<li>CA_Stew</li>
		</researchPrerequisites>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
		</stuffCategories>
		<costStuffCount>20</costStuffCount>
		<rotatable>false</rotatable>
		<tickerType>Normal</tickerType>
	</ThingDef>

