<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<RecipeDef>
		<defName>CA_MakeCheese</defName>
		<label>make cheese</label>
		<description>Make cheese.</description>
		<jobString>Making cheese.</jobString>
		<workAmount>800</workAmount>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Milk</li>
					</thingDefs>
				</filter>
				<count>1.0</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>CA_Vinegar</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Milk</li>
				<li>CA_Vinegar</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<CA_Cheese>25</CA_Cheese>
		</products>
		<recipeUsers>
			<li>Campfire</li>
			<li>ElectricStove</li>
			<li>FueledStove</li>
		</recipeUsers>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<requiredGiverWorkType>Cooking</requiredGiverWorkType>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
	</RecipeDef>

	<RecipeDef>
		<defName>CA_MakeJam</defName>
		<label>make jam</label>
		<description>Make jam.</description>
		<jobString>Making jam.</jobString>
		<workAmount>800</workAmount>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>CA_Fruit</li>
					</categories>
					<thingDefs>
						<li>RawBerries</li>
					</thingDefs>
				</filter>
				<count>1.0</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>CA_Sugar</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>CA_Fruit</li>
			</categories>
			<thingDefs>
				<li>RawBerries</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<CA_Jam>25</CA_Jam>
		</products>
		<recipeUsers>
			<li>Campfire</li>
			<li>ElectricStove</li>
			<li>FueledStove</li>
		</recipeUsers>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<requiredGiverWorkType>Cooking</requiredGiverWorkType>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
	</RecipeDef>

	<ProcessorFramework.ProcessDef>
		<defName>CA_Smoking</defName>
		<thingDef>CA_SmokedMeat</thingDef>
		<ingredientFilter>
			<categories>
				<li>MeatRaw</li>
			</categories>
		</ingredientFilter>
		<processDays>3</processDays>
		<unfueledFactor>0.0</unfueledFactor>
		<fuelUseFactor>500.0</fuelUseFactor>
	</ProcessorFramework.ProcessDef>

	<!-- Salt -->
	<RecipeDef>
		<defName>CA_MakeSaltFromChunk</defName>
		<label>make salt from rock salt</label>
		<description>Salt.</description>
		<jobString>Making salt from rock salt.</jobString>
		<workAmount>800</workAmount>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<effectWorking>CutStone</effectWorking>
		<soundWorking>Recipe_MakeStoneBlocks</soundWorking>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>CA_ChunkSalt</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<products>
			<CA_Salt>10</CA_Salt>
		</products>
		<recipeUsers>
			<li>TableStonecutter</li>
		</recipeUsers>
	</RecipeDef>

	<RecipeDef>
		<defName>CA_SaltVegetable</defName>
		<label>salt vegetables</label>
		<description>Salt vegetables.</description>
		<jobString>Salting vegetables.</jobString>
		<workAmount>800</workAmount>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>PlantFoodRaw</li>
					</categories>
				</filter>
				<count>1.0</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>CA_Salt</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>PlantFoodRaw</li>
			</categories>
			<disallowedCategories>
				<li>CA_Fruit</li>
			</disallowedCategories>
			<disallowedThingDefs>
				<li>RawBerries</li>
				<li>CA_Jam</li>
				<li>CA_SaltedVegetable</li>
				<li>CA_PickledVegetable</li>
			</disallowedThingDefs>
		</fixedIngredientFilter>
		<products>
			<CA_SaltedVegetable>25</CA_SaltedVegetable>
		</products>
		<recipeUsers>
			<li>Campfire</li>
			<li>ElectricStove</li>
			<li>FueledStove</li>
		</recipeUsers>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<requiredGiverWorkType>Cooking</requiredGiverWorkType>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
	</RecipeDef>

	<ProcessorFramework.ProcessDef>
		<defName>CA_Pickling</defName>
		<thingDef>CA_PickledVegetable</thingDef>
		<ingredientFilter>
			<thingDefs>
				<li>CA_SaltedVegetable</li>
			</thingDefs>
		</ingredientFilter>
		<processDays>3</processDays>
		<bonusOutputs>
			<CA_Vinegar>(1.0, 10)</CA_Vinegar>
		</bonusOutputs>
	</ProcessorFramework.ProcessDef>

	<!-- Sugar -->
	<RecipeDef>
		<defName>CA_MakeSyrup</defName>
		<label>make syrup</label>
		<description>Make syrup.</description>
		<jobString>Making syrup.</jobString>
		<workAmount>800</workAmount>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>CA_Sugarcane</li>
					</thingDefs>
				</filter>
				<count>25</count>
			</li>
		</ingredients>
		<products>
			<CA_Syrup>25</CA_Syrup>
		</products>
		<recipeUsers>
			<li>Campfire</li>
			<li>ElectricStove</li>
			<li>FueledStove</li>
		</recipeUsers>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<requiredGiverWorkType>Cooking</requiredGiverWorkType>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
	</RecipeDef>

	<ProcessorFramework.ProcessDef>
		<defName>CA_SugarEvaporation</defName>
		<thingDef>CA_Sugar</thingDef>
		<ingredientFilter>
			<thingDefs>
				<li>CA_Syrup</li>
			</thingDefs>
		</ingredientFilter>
		<processDays>0.67</processDays>
		<temperatureIdeal>10~100</temperatureIdeal>
		<temperatureSafe>-50~200</temperatureSafe>
		<sunFactor>0.4~1.0</sunFactor>
		<rainFactor>1~0</rainFactor>
		<snowFactor>1~0</snowFactor>
		<windFactor>0.8~1.0</windFactor>
	</ProcessorFramework.ProcessDef>

	<ProcessorFramework.ProcessDef>
		<defName>CA_SugarEvaporationCentrifuge</defName>
		<thingDef>CA_Sugar</thingDef>
		<ingredientFilter>
			<thingDefs>
				<li>CA_Syrup</li>
			</thingDefs>
		</ingredientFilter>
		<processDays>0.5</processDays>
		<usesTemperature>false</usesTemperature>
		<unpoweredFactor>0.0</unpoweredFactor>
		<powerUseFactor>8.0</powerUseFactor>
	</ProcessorFramework.ProcessDef>

	<ProcessorFramework.ProcessDef>
		<defName>CA_SugarGrinding</defName>
		<thingDef>CA_Syrup</thingDef>
		<ingredientFilter>
			<thingDefs>
				<li>CA_Sugarcane</li>
			</thingDefs>
		</ingredientFilter>
		<efficiency>0.333</efficiency>
		<processDays>0.5</processDays>
		<usesTemperature>false</usesTemperature>
		<unpoweredFactor>0.0</unpoweredFactor>
		<powerUseFactor>8.0</powerUseFactor>
	</ProcessorFramework.ProcessDef>

	<!-- Milling -->
	<RecipeDef>
		<defName>CA_MakeFlourAtQuern</defName>
		<label>grind flour from wheat</label>
		<description>Grind flour from wheat by hand at a quern.</description>
		<jobString>Grinding flour.</jobString>
		<workAmount>800</workAmount>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>CA_Wheat</li>
					</thingDefs>
				</filter>
				<count>20</count>
			</li>
		</ingredients>
		<products>
			<CA_Flour>20</CA_Flour>
		</products>
		<recipeUsers>
			<li>CA_MillManual</li>
		</recipeUsers>
	</RecipeDef>

	<ProcessorFramework.ProcessDef>
		<defName>CA_MillingWind</defName>
		<thingDef>CA_Flour</thingDef>
		<ingredientFilter>
			<thingDefs>
				<li>CA_Wheat</li>
			</thingDefs>
		</ingredientFilter>
		<processDays>0.67</processDays>
		<usesTemperature>false</usesTemperature>
		<windFactor>0~1.0</windFactor>
	</ProcessorFramework.ProcessDef>

	<ProcessorFramework.ProcessDef>
		<defName>CA_MillingElectric</defName>
		<thingDef>CA_Flour</thingDef>
		<ingredientFilter>
			<thingDefs>
				<li>CA_Wheat</li>
			</thingDefs>
		</ingredientFilter>
		<processDays>0.5</processDays>
		<usesTemperature>false</usesTemperature>
		<unpoweredFactor>0.0</unpoweredFactor>
		<powerUseFactor>8.0</powerUseFactor>
	</ProcessorFramework.ProcessDef>
</Defs>
